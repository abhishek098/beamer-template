	title = {{Preprint of " Reinforcement Learning with Sequences of Motion Primitives for Robust Manipulation " Reinforcement Learning with Sequences of Motion Primitives for Robust Manipulation}},
	volume = {28},
	year = {2012}
}
@inproceedings{Ijspeert,
	abstract = {Presents an approach to movement planning, on-line trajectory$\backslash$nmodification, and imitation learning by representing movement plans$\backslash$nbased on a set of nonlinear differential equations with well-defined$\backslash$nattractor dynamics. The resultant movement plan remains an autonomous$\backslash$nset of nonlinear differential equations that forms a control policy (CP)$\backslash$nwhich is robust to strong external perturbations and that can be$\backslash$nmodified on-line by additional perceptual variables. We evaluate the$\backslash$nsystem with a humanoid robot simulation and an actual humanoid robot.$\backslash$nExperiments are presented for the imitation of three types of movements:$\backslash$nreaching movements with one arm, drawing movements of 2-D patterns, and$\backslash$ntennis swings. Our results demonstrate (a) that multi-joint human$\backslash$nmovements can be encoded successfully by the CPs, (b) that a learned$\backslash$nmovement policy can readily be reused to produce robust trajectories$\backslash$ntowards different targets, (c) that a policy fitted for one particular$\backslash$ntarget provides a good predictor of human reaching movements towards$\backslash$nneighboring targets, and (d) that the parameter space which encodes a$\backslash$npolicy is suitable for measuring to which extent two trajectories are$\backslash$nqualitatively similar},
