@article{Chhabra2008,
	abstract = {The computational complexities arising in motor control can be$\backslash$nameliorated through the use of a library of motor synergies. We$\backslash$npresent a new model, referred to as the Greedy Additive Regression$\backslash$n(GAR) model, for learning a library of torque sequences, and for$\backslash$nlearning the coefficients of a linear combination of sequences$\backslash$nminimizing a cost function. From the perspective of numerical$\backslash$noptimization, the GAR model is interesting because it creates a$\backslash$nlibrary of "local features"---each sequence in the library is a$\backslash$nsolution to a single training task---and learns to combine these$\backslash$nsequences using a local optimization procedure, namely, additive$\backslash$nregression. We speculate that learners with local representational$\backslash$nprimitives and local optimization procedures will show good$\backslash$nperformance on nonlinear tasks. The GAR model is also interesting$\backslash$nfrom the perspective of motor control because it outperforms several$\backslash$ncompeting models. Results using a simulated two-joint arm suggest$\backslash$nthat the GAR model consistently shows excellent performance in the$\backslash$nsense that it rapidly learns to perform novel, complex motor tasks.$\backslash$nMoreover, its library is overcomplete and sparse, meaning that only$\backslash$na small fraction of the stored torque sequences are used when$\backslash$nlearning a new movement. The library is also robust in the sense$\backslash$nthat, after an initial training period, nearly all novel movements$\backslash$ncan be learned as additive combinations of sequences in the library,$\backslash$nand in the sense that it shows good generalization when an arm's$\backslash$ndynamics are altered between training and test conditions, such as$\backslash$nwhen a payload is added to the arm. Lastly, the GAR model works well$\backslash$nregardless of whether motor tasks are specified in joint space or$\backslash$nCartesian space. We conclude that learning techniques using local$\backslash$nprimitives and optimization procedures are viable and potentially$\backslash$nimportant methods for motor control and possibly other domains, and$\backslash$nthat these techniques deserve further examination by the artificial$\backslash$nintelligence and cognitive science communities.},
	author = {Chhabra, Manu and Jacobs, Robert A.},
	isbn = {1532-4435},
	issn = {15324435},
	journal = {Journal of Machine Learning Research},
	keywords = {additive regression,motor primitives,sparse representations},
	pages = {1535----1558},
	title = {{Learning to Combine Motor Primitives Via Greedy Additive Regression}},
	url = {http://jmlr.csail.mit.edu/papers/v9/chhabra08a.html{\%}5Cnhttp://www.jmlr.org/papers/volume9/chhabra08a/chhabra08a.pdf},
	volume = {9},
	year = {2008}
}

@article{Pastor2013,
	abstract = {In recent years, research on movement primitives has gained increasing popularity. The original goals of movement primitives are based on the desire to have a sufficiently rich and abstract representation for movement generation, which allows for efficient teaching, trial-and-error learning, and generalization of motor skills (Schaal 1999). Thus, motor skills in robots should be acquired in a natural dialog with humans, e.g., by imitation learning and shaping, while skill refinement and generalization should be accomplished autonomously by the robot. Such a scenario resembles the way we teach children and connects to the bigger question of how the human brain accomplishes skill learning. In this paper, we review how a particular computational approach to movement primitives, called dynamic movement primitives, can contribute to learning motor skills. We will address imitation learning, generalization, trial-and-error learning by reinforcement learning, movement recognition, and control based on movement primitives. But we also want to go beyond the standard goals of movement primitives. The stereotypical movement generation with movement primitives entails predicting of sensory events in the environment. Indeed, all the sensory events associated with a movement primitive form an associative skill memory that has the potential of forming a most powerful representation of a complete motor skill. {\textcopyright} 2012 Elsevier B.V. All rights reserved.},
	author = {Pastor, Peter and Kalakrishnan, Mrinal and Meier, Franziska and Stulp, Freek and Buchli, Jonas and Theodorou, Evangelos and Schaal, Stefan},
	doi = {10.1016/j.robot.2012.09.017},
	isbn = {0921-8890},
	issn = {09218890},
	journal = {Robotics and Autonomous Systems},
	keywords = {Associative skill memories,Dynamic systems,Generalization,Learning,Movement primitives},
	number = {4},
	pages = {351--361},
	publisher = {Elsevier},
	title = {{From dynamic movement primitives to associative skill memories}},
	volume = {61},
	year = {2013}
}
@article{Schaal1999,
	abstract = {This review investigates two recent developments in artificial intelligence and neural computation: learning from imitation and the development of humanoid robots. It is postulated that the study of imitation learning offers a promising route to gain new insights into mechanisms of perceptual motor control that could ultimately lead to the creation of autonomous humanoid robots. Imitation learning focuses on three important issues: efficient motor learning, the connection between action and perception, and modular motor control in the form of movement primitives. It is reviewed here how research on representations of, and functional connections between, action and perception have contributed to our understanding of motor acts of other beings. The recent discovery that some areas in the primate brain are active during both movement perception and execution has provided a hypothetical neural basis of imitation. Computational approaches to imitation learning are also described, initially from the perspective of traditional Al and robotics, but also from the perspective of neural network models and statistical-learning research. Parallels and differences between biological and computational approaches to imitation are highlighted and an overview of current projects that actually employ imitation learning for humanoid robots is given.},
	author = {Schaal, Stefan},
	doi = {10.1016/S1364-6613(99)01327-3},
	isbn = {1364-6613},
	issn = {13646613},
	journal = {Trends in cognitive sciences},
	number = {6},
	pages = {233--242},
	pmid = {10354577},
	publisher = {Elsevier},
	title = {{Is imitation learning the route to humanoid robots?}},
	volume = {3},
	year = {1999}
}
@inproceedings{Kormushev2010,
	abstract = {We present an approach allowing a robot to acquire new motor skills by learning the couplings across motor control variables. The demonstrated skill is first encoded in a compact form through a modified version of Dynamic Movement Primitives (DMP) which encapsulates correlation information. Expectation-Maximization based Reinforcement Learning is then used to modulate the mixture of dynamical systems initialized from the user's demonstration. The approach is evaluated on a torque-controlled 7 DOFs Barrett WAM robotic arm. Two skill learning experiments are conducted: a reaching task where the robot needs to adapt the learned movement to avoid an obstacle, and a dynamic pancake-flipping task.},
