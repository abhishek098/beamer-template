	journal = {Foundations and Trends in Robotics},
	number = {1-2},
	pages = {1--142},
	title = {{A Survey on Policy Search for Robotics}},
	url = {http://www.nowpublishers.com/articles/foundations-and-trends-in-robotics/ROB-021},
	volume = {2},
	year = {2011}
}
